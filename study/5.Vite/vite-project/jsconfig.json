{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~/*": [
                "src/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@/*": [
                "src/*"
            ],
            "!assets/*": [
                "src/assets/*"
            ],
            "!components/*": [
                "src/components/*"
            ],
            "!/*": [
                "src/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ]
        }
    }
}